	"autoload-dev": {
		"psr-4": {
			"Lemuria\\Tests\\": "vendor/lemuria-pbem/lemuria/tests/",
			"Lemuria\\Tests\\Model\\Fantasya\\": "tests/"
		},
		"files": ["vendor/lemuria-pbem/lemuria/src/Lemuria.php"]
